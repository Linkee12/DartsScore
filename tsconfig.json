{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}
